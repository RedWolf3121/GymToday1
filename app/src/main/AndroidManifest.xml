<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymtoday1">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DietasActivity"
            android:label="@string/title_activity_dietas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EjerciciosActivity"
            android:label="@string/title_activity_ejercicios"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EjerciciosEspaldaActivity"
            android:label="@string/title_activity_ejercicios_espalda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EjerciciosLunesActivity"
            android:label="@string/title_activity_ejercicios_lunes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FavouritesActivity"
            android:label="@string/title_activity_favourites"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".KCAL_1200_Activity"
            android:label="@string/title_activity_kcal_1200_"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RutinaActivity"
            android:label="@string/title_activity_rutina"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Ejercicio1EspaldaActivity"
            android:label="@string/title_activity_ejercicio1_espalda"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>